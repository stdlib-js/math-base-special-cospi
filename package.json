{
  "name": "@stdlib/math-base-special-cospi",
  "version": "0.4.0",
  "description": "Compute cos(πx).",
  "license": "Apache-2.0",
  "author": {
    "name": "The Stdlib Authors",
    "url": "https://github.com/stdlib-js/stdlib/graphs/contributors"
  },
  "contributors": [
    {
      "name": "The Stdlib Authors",
      "url": "https://github.com/stdlib-js/stdlib/graphs/contributors"
    }
  ],
  "main": "./lib",
  "gypfile": false,
  "directories": {
    "benchmark": "./benchmark",
    "doc": "./docs",
    "example": "./examples",
    "include": "./include",
    "lib": "./lib",
    "src": "./src",
    "test": "./test"
  },
  "types": "./docs/types",
  "scripts": {
    "test": "make test",
    "test-cov": "make test-cov",
    "examples": "make examples",
    "benchmark": "make benchmark"
  },
  "homepage": "https://stdlib.io",
  "repository": {
    "type": "git",
    "url": "git://github.com/stdlib-js/math-base-special-cospi.git"
  },
  "bugs": {
    "url": "https://github.com/stdlib-js/stdlib/issues"
  },
  "dependencies": {
    "@stdlib/constants-float64-max-safe-integer": "^0.2.2",
    "@stdlib/constants-float64-pi": "^0.2.2",
    "@stdlib/math-base-assert-is-infinite": "^0.2.2",
    "@stdlib/math-base-assert-is-nan": "^0.2.2",
    "@stdlib/math-base-napi-unary": "^0.2.3",
    "@stdlib/math-base-special-abs": "^0.2.2",
    "@stdlib/math-base-special-cos": "^0.3.0",
    "@stdlib/math-base-special-floor": "^0.2.3",
    "@stdlib/math-base-special-fmod": "^0.1.0",
    "@stdlib/math-base-special-sin": "^0.3.0",
    "@stdlib/utils-library-manifest": "^0.2.2"
  },
  "devDependencies": {
    "@stdlib/array-base-incrspace": "^0.2.2",
    "@stdlib/assert-is-positive-zero": "^0.2.2",
    "@stdlib/console-log-each-map": "github:stdlib-js/console-log-each-map#main",
    "@stdlib/constants-float64-eps": "^0.2.2",
    "@stdlib/constants-float64-ninf": "^0.2.2",
    "@stdlib/constants-float64-pinf": "^0.2.2",
    "@stdlib/math-base-special-pow": "^0.3.0",
    "@stdlib/random-array-uniform": "^0.2.1",
    "@stdlib/utils-try-require": "^0.2.2",
    "tape": "git+https://github.com/kgryte/tape.git#fix/globby",
